{
  "name": "vue-web-terminal",
  "version": "3.0.5",
  "description": "网页端命令行插件，适配vue3",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-cli": "vue-cli-service build --target lib --dest lib src/index.js && python modify_lib.py",
    "build-wc": "vue-cli-service build --target wc --dest lib src/Terminal.vue",
    "build": "webpack --mode=production --config webpack.config.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "object-sizeof": "^1.6.2",
    "vue-json-viewer": "^3.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/compiler-dom": "^3.2.31",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "compression-webpack-plugin": "^9.2.0",
    "core-js": "^3.8.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "terser-webpack-plugin": "^5.3.1",
    "vue": "^3.2.31",
    "vue-loader": "^16.8.3",
    "style-loader": "^3.3.1",
    "webpack": "^5.69.0",
    "webpack-cli": "^4.9.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tzfun/vue-web-terminal.git"
  },
  "bugs": {
    "url": "https://github.com/tzfun/vue-web-terminal/issues"
  },
  "homepage": "https://github.com/tzfun/vue-web-terminal#readme",
  "keywords": [
    "web-terminal",
    "terminal",
    "console",
    "web-console",
    "vue-terminal",
    "vue-console"
  ],
  "author": "beifengtz",
  "main": "lib/vue-web-terminal.js"
}
