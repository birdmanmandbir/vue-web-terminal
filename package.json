{
  "name": "vue-web-terminal",
  "version": "3.1.3",
  "description": "A lightweight and powerful web terminal plugin, suitable for vue3. 轻量、功能强大的网页端Terminal插件，适配vue3",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "dev": "vite",
    "dev:demo": "npm run build && npm run dev",
    "build": "vite build && npm run build:types",
    "preview": "vite preview",
    "build:types": "vue-tsc --project tsconfig.json --declaration --emitDeclarationOnly --outDir lib/types"
  },
  "peerDependencies": {
    "vue": "^3.x"
  },
  "dependencies": {
    "@vueuse/core": "^9.8.1",
    "lodash": "^4.17.21",
    "vue3-json-viewer": "^2.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.30.0",
    "eslint-plugin-vue": "^9.8.0",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.1",
    "vite": "^4.0.2",
    "vite-plugin-eslint": "^1.8.1",
    "vue": "^3.2.31",
    "vue-tsc": "^1.0.13"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tzfun/vue-web-terminal.git"
  },
  "bugs": {
    "url": "https://github.com/tzfun/vue-web-terminal/issues"
  },
  "homepage": "https://tzfun.github.io/vue-web-terminal/",
  "keywords": [
    "terminal",
    "vue",
    "vue3",
    "web-terminal",
    "vue-terminal",
    "console",
    "web-console",
    "vue-console"
  ],
  "author": "beifengtz",
  "main": "./lib/vue-web-terminal.umd.js",
  "module": "./lib/vue-web-terminal.es.js",
  "types": "./lib/types/index.d.ts",
  "style": "./lib/style.css",
  "exports": {
    ".": {
      "import": "./lib/vue-web-terminal.es.js",
      "require": "./lib/vue-web-terminal.umd.js"
    },
    "./lib/style.css": "./lib/style.css"
  },
  "files": [
    "dist"
  ]
}
